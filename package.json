{
  "name": "ghpage",
  "version": "1.0.0",
  "description": "ghpages source code",
  "main": "bundle.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "get_assets": "./devtools/get_assets.rb",
    "build": "npm run get_assets && npm run make_conf && webpack --config ./webpack.config.js",
    "watch": "npm run make_conf && webpack --watch",
    "build_rl": "npm run get_assets && npm run make_conf && webpack --config ./webpack-release.config.js",
    "make_conf": "./devtools/yml2json.rb ./tsconfig.yaml > ./tsconfig.json && ./devtools/yml2json.rb ./docs/oss_contribution.yaml > ./docs/oss_contribution.json && ./devtools/yml2json.rb ./docs/tool_tips.yaml > ./docs/tool_tips.json",
    "webpack_analyze": "npm run get_assets && npm run make_conf && webpack --config ./webpack-analyze.config.js"
  },
